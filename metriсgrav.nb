Cell[TextData[{
 "Given a n-dimensional manifold \[ScriptCapitalM] whose coordinates we \
denote ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "i"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " and a metric defined over it and provided in the form"
}], "Text",
 CellChangeTimes->{{3.7079031254058466`*^9, 3.7079031259373226`*^9}},
 FontSize->14],

Cell[BoxData[
 FormBox[
  RowBox[{
  "                                                                           \
                                              ", 
   RowBox[{
    SuperscriptBox["ds", "2"], "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox["g", "ij"], "(", "x", ")"}], " ", 
     RowBox[{
      SuperscriptBox["dx", "i"], "\[CircleTimes]", 
      SuperscriptBox["dx", "j"]}]}]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{
  3.707902938752051*^9, {3.7079030101233664`*^9, 3.7079030982143536`*^9}},
 FontSize->14],

Cell[TextData[{
 "The programme extracts the metric tensor ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["g", "ij"], 
   RowBox[{"(", "x", ")"}], " "}]],
  CellChangeTimes->{
   3.707902938752051*^9, {3.7079030101233664`*^9, 3.7079030982143536`*^9}},
  FontSize->14],
 "calculates its inverse ",
 Cell[BoxData[
  RowBox[{
   SuperscriptBox["g", "ij"], 
   RowBox[{"(", "x", ")"}]}]],
  CellChangeTimes->{
   3.707902938752051*^9, {3.7079030101233664`*^9, 3.7079030982143536`*^9}},
  FontSize->14],
 "calculates the Christoffel symbols ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    SubscriptBox["\[CapitalGamma]", "ij"], "k"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "(x), then the Riemann tensor, the Ricci tensor and the Einstein tensor."
}], "Text",
 CellChangeTimes->{{3.70790310415296*^9, 3.7079032891186085`*^9}},
 FontSize->14],

