Cell[CellGroupData[{

Cell["routine mainmetric ", "Subsection",
 CellChangeTimes->{{3.707902537483544*^9, 3.707902553080083*^9}},
 FontSize->14],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"mainmetric", ":=", 
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
      "Print", "[", "\"\<OK I calculate your space, Give me the data\>\"", 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{
      "Print", "[", "\"\<Give me the dimension of your space\>\"", "]"}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"mdim", "=", "nn"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Your space has dimension n = \>\"", ",", "mdim"}], "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{
       "\"\<Now I stop and you give me two vectors of dimension \>\"", ",", 
        "mdim"}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{
      "Print", "[", "\"\<vector coordi = vector of coordinates\>\"", "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{
      "Print", "[", "\"\<vector diffe = vector of differentials\>\"", "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{
      "Print", "[", "\"\<Next you give me the metric as ds2 = \>\"", "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{
      "Print", "[", 
       "\"\<Then to resume calculation you print metricresume\>\"", "]"}], 
      ";"}], "\[IndentingNewLine]", "}"}]}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{
  3.707902571348446*^9, {3.7079047595820384`*^9, 3.7079047600042324`*^9}},
 FontSize->14]
}, Closed]],

Cell[CellGroupData[{

Cell["The routine firstres", "Subsection",
 CellChangeTimes->{{3.70790259829051*^9, 3.7079026121054173`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"firstres", ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Print", "[", "\"\<I resume the calculation\>\"", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
     "Print", "[", 
      "\"\<First I extract the metric coefficients from your data\>\"", "]"}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"gg", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "mdim"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "mdim"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Do", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              SubscriptBox["gg", 
               RowBox[{"\[LeftDoubleBracket]", 
                RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}]], "=", 
              RowBox[{
               FractionBox["1", "2"], 
               RowBox[{"(", 
                RowBox[{"Coefficient", "[", 
                 RowBox[{"ds2", ",", 
                  RowBox[{
                   SubscriptBox["diffe", 
                    RowBox[{
                    "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]], 
                   "*", 
                   SubscriptBox["diffe", 
                    RowBox[{
                    "\[LeftDoubleBracket]", "j", 
                    "\[RightDoubleBracket]"}]]}]}], "]"}], ")"}]}]}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{
              SubscriptBox["gg", 
               RowBox[{"\[LeftDoubleBracket]", 
                RowBox[{"j", ",", "i"}], "\[RightDoubleBracket]"}]], "=", 
              RowBox[{
               FractionBox["1", "2"], 
               RowBox[{"(", 
                RowBox[{"Coefficient", "[", 
                 RowBox[{"ds2", ",", 
                  RowBox[{
                   SubscriptBox["diffe", 
                    RowBox[{
                    "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]], 
                   "*", 
                   SubscriptBox["diffe", 
                    RowBox[{
                    "\[LeftDoubleBracket]", "j", 
                    "\[RightDoubleBracket]"}]]}]}], "]"}], ")"}]}]}], ";"}], 
            "\[IndentingNewLine]", "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", 
             RowBox[{"i", "+", "1"}], ",", "mdim"}], "}"}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{
          SubscriptBox["gg", 
           RowBox[{"\[LeftDoubleBracket]", 
            RowBox[{"i", ",", "i"}], "\[RightDoubleBracket]"}]], "=", 
          RowBox[{"Coefficient", "[", 
           RowBox[{"ds2", ",", 
            SuperscriptBox[
             SubscriptBox["diffe", 
              RowBox[{"\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]],
              "2"]}], "]"}]}], ";"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "mdim"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
     "Print", "[", "\"\<Then I calculate the inverse metric\>\"", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ggm", "=", 
      RowBox[{"Simplify", "[", 
       RowBox[{"Inverse", "[", "gg", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", "\"\<Done !\>\"", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
     "Print", "[", "\"\<and I calculate also the metric determinant\>\"", 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"detto", "=", 
      RowBox[{"Simplify", "[", 
       RowBox[{"Det", "[", "gg", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", "\"\<Done\>\"", "]"}], ";"}], "}"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{3.707902574552174*^9}]
}, Closed]],

Cell[CellGroupData[{

Cell["metricresume", "Subsection",
