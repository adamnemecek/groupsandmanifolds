      RowBox[{"lung", "=", 
       RowBox[{"Length", "[", "gruppone", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<The order of this group is = \>\"", ",", "lung"}], "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"gruppo", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         SubscriptBox["gruppone", 
          RowBox[{"\[LeftDoubleBracket]", "xx", "\[RightDoubleBracket]"}]], 
         ",", 
         RowBox[{"{", 
          RowBox[{"xx", ",", "1", ",", "lung"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"loppo", "=", "gruppo"}], ";", "\[IndentingNewLine]", 
      RowBox[{"dimens", "=", 
       RowBox[{"Length", "[", "gruppo", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"classa", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
