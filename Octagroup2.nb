Cell[BoxData[
 RowBox[{"orbitandus", ":=", 
  RowBox[{"{", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Print", "[", "\"\<========================\>\"", "]"}], ";", 
    RowBox[{
    "Print", "[", 
     "\"\<This is the routine orbitandus which decomposes the spherical \
layers into orbits\>\"", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Print", "[", "\"\<-------------------------\>\"", "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"rascutto", "=", 
     RowBox[{"Length", "[", "multip", "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"radione", "=", 
     RowBox[{"multip", "[", 
      RowBox[{"[", 
       RowBox[{"rascutto", ",", "1"}], "]"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"filtro", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", 
         RowBox[{"{", "}"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"ss", ",", "1", ",", "rascutto"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"kk", "=", "0"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Label", "[", "carciofo", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"kk", "\[Equal]", "rascutto"}], ",", 
      RowBox[{"Goto", "[", "stuppo", "]"}], ",", 
      RowBox[{"pippo", "=", "0"}]}], "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"kk", "=", 
     RowBox[{"kk", "+", "1"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"UUU", "=", 
     SubscriptBox["strata", 
      RowBox[{"\[LeftDoubleBracket]", "kk", "\[RightDoubleBracket]"}]]}], ";",
     "\[IndentingNewLine]", 
    RowBox[{"xxy", "=", "0"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Label", "[", "carota", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"UUU", "\[Equal]", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"Goto", "[", "carciofo", "]"}], ",", 
      RowBox[{"xxy", "=", 
       RowBox[{"xxy", "+", "1"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"vec", "=", 
     SubscriptBox["UUU", 
      RowBox[{"\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]]}], ";", 
    "\[IndentingNewLine]", "genorbP", ";", "\[IndentingNewLine]", 
    RowBox[{"lin", "=", 
     RowBox[{"Length", "[", "orbita", "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     SubscriptBox["filtro", 
      RowBox[{"\[LeftDoubleBracket]", 
       RowBox[{"kk", ",", "1"}], "\[RightDoubleBracket]"}]], "=", 
     SubscriptBox["multip", 
      RowBox[{"\[LeftDoubleBracket]", 
       RowBox[{"kk", ",", "1"}], "\[RightDoubleBracket]"}]]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
     SubscriptBox["filtro", 
      RowBox[{"\[LeftDoubleBracket]", 
       RowBox[{"kk", ",", "2"}], "\[RightDoubleBracket]"}]], "=", 
     SubscriptBox["multip", 
      RowBox[{"\[LeftDoubleBracket]", 
       RowBox[{"kk", ",", "2"}], "\[RightDoubleBracket]"}]]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
     SubscriptBox["filtro", 
      RowBox[{"\[LeftDoubleBracket]", 
       RowBox[{"kk", ",", "3"}], "\[RightDoubleBracket]"}]], "=", 
