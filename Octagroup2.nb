      "\[IndentingNewLine]", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"bullo", "=", 
           RowBox[{"Simplify", "[", 
            RowBox[{
             RowBox[{"Inverse", "[", 
              SubscriptBox["gruppo", 
               RowBox[{
               "\[LeftDoubleBracket]", "tt", "\[RightDoubleBracket]"}]], 
              "]"}], ".", 
             SubscriptBox["loppo", 
              RowBox[{"\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]],
              ".", 
             SubscriptBox["gruppo", 
              RowBox[{
              "\[LeftDoubleBracket]", "tt", "\[RightDoubleBracket]"}]]}], 
            "]"}]}], ";", 
          RowBox[{"kussa", "=", 
           RowBox[{"kussa", "\[Union]", 
