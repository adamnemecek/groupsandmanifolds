Cell[BoxData[
 RowBox[{"latticePS", ":=", 
  RowBox[{"{", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
    "Print", "[", "\"\<================================================\>\"", 
     "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{
    "Print", "[", "\"\<-------------------------------------------\>\"", 
     "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{
    "Print", "[", 
     "\"\<This is the routine to generate the Self Dual Cubic Lattice \
points\>\"", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"aa", "=", "spaz"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{
     "\"\< You gave me the lattice spacing aa = \>\"", ",", " ", "aa"}], 
     "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{
     "\"\<You also gave me the number of points in the cubic rays nn = \>\"", 
      ",", "nplan"}], "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Print", "[", "\"\< First I generate the   cubic rays\>\"", "]"}],
     ";", "\[IndentingNewLine]", 
    RowBox[{
    "Print", "[", "\"\<-------------------------------------------\>\"", 
     "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{
     "\"\< Your point number on the cubic rays is = \>\"", ",", "nplan"}], 
     "]"}], ";", "\n", 
    RowBox[{"v1", "=", 
     RowBox[{"aa", "*", 
      RowBox[{"{", 
       RowBox[{"1", ",", "0", ",", "0"}], "}"}]}]}], ";", 
    RowBox[{"v2", "=", 
     RowBox[{"aa", "*", 
      RowBox[{"{", 
       RowBox[{"0", ",", "1", ",", "0"}], "}"}]}]}], ";", 
    RowBox[{"v3", "=", 
     RowBox[{"aa", "*", 
      RowBox[{"{", 
       RowBox[{"0", ",", "0", ",", "1"}], "}"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
    "Print", "[", "\"\<The basis vectors of your lattice are\>\"", "]"}], ";",
     "\[IndentingNewLine]", 
    RowBox[{"ww", "=", 
     RowBox[{"{", 
      RowBox[{"v1", ",", "v2", ",", "v3"}], "}"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{
      RowBox[{"MatrixForm", "[", 
       RowBox[{"{", 
        RowBox[{
         SubscriptBox["e", "1"], ",", 
         SubscriptBox["e", "2"], ",", 
         SubscriptBox["e", "3"]}], "}"}], "]"}], ",", "\"\< = \>\"", ",", 
      RowBox[{"MatrixForm", "[", "ww", "]"}]}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Print", "[", "\"\<The number of points per line are\>\"", "]"}], 
    ";", "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{"\"\< number of points per line = \>\"", ",", 
      RowBox[{
       RowBox[{"2", "*", "nplan"}], "+", "1"}]}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Print", "[", "\"\<I generate\>\"", "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"lattex", "=", 
     RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"Do", "[", 
        RowBox[{
         RowBox[{"Do", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"vetto", "=", 
              RowBox[{"Simplify", "[", 
